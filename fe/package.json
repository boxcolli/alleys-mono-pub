{
  "name": "fe",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "preview": "bun run build && wrangler pages dev --live-reload true",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "email": "mjml email/code.mjml -o email/code.html --config.minify && mjml email/already.mjml -o email/already.html --config.minify"
  },
  "dependencies": {
    "@alleys/api": "workspace:*",
    "@alleys/azure": "workspace:*",
    "@alleys/cf": "workspace:*",
    "@alleys/kanel": "workspace:*",
    "@alleys/kysely": "workspace:*",
    "@alleys/lib": "workspace:*",
    "@alleys/util": "workspace:*",
    "@marsidev/react-turnstile": "^1.1.0",
    "@noble/hashes": "^1.6.1",
    "@remix-run/cloudflare": "^2.15.0",
    "@remix-run/cloudflare-pages": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@rvf/remix": "^6.3.0",
    "@rvf/zod": "^6.0.3",
    "@stdlib/string-format": "^0.2.2",
    "@wcj/html-to-markdown": "^2.1.1",
    "beercss": "^3.7.14",
    "es-hangul": "^2.2.3",
    "isbot": "^4.4.0",
    "material-dynamic-colors": "^1.1.2",
    "mustache": "^4.2.0",
    "open-props": "^1.7.8",
    "pino": "^9.5.0",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "swiper": "^11.1.15",
    "tiny-invariant": "^1.3.3",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@remix-run/dev": "^2.15.0",
    "@types/mustache": "^4.2.5",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.49",
    "postcss-custom-media": "^11.0.5",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wrangler": "^3.92.0",
